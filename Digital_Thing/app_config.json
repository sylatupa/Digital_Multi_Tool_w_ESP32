{
  "this_thing":
  {
    "wifi": {
      "ssid": null
    },
    "events": [
      {"id":"thing_id"},
      {"id":"wifi/ssid"}
    ],
    "subscribes": [
      {"id":"osc_status"},
      {"id":"mqtt_status"},
      {"id":"udp_status"},
      {"id":"behavior_topics"}
    ],
    "behavior_topics": "on"
  }
  ,
  "touchpins":{
    "freq" : "1",
    "events": [
      {"id":"up","pin":12},
      {"id":"left","pin":13},
      {"id":"right","pin":14},
      {"id":"down","pin":15}
    ]
  },
  "onboard_temp":{
    "freq" : "1",
    "events": [
      {"id":"temp"}
    ],
    "subscribes": [
      {"id":"sample_rate"},
      {"id":"trigger"},
      {"id":"threshold_above"},
      {"id":"threshold_below"}
    ]
  } ,
  "magnetic_field":{
    "freq" : "1",
    "events": [
      {"id":"hall","pin":21}
    ],
    "subscribes": [
      {"id":"sample_rate"},
      {"id":"trigger_threshold"}
    ]
  } ,

  "spirit_level":{
    "freq" : "1",
    "events": [
      {"id":"x","pin":31},
      {"id":"y","pin":32},
      {"id":"z","pin":33}
    ],
    "subscribes": [
      {"id":"sample_rate"},
      {"id":"trigger_threshold"}
    ]
  },
  "haptic_feedback":{
    "freq" : "1",
    "subscribes": [
      {"id":"hall"},
      {"id":"trigger"},
      {"id":"sample_rate"},
      {"id":"x","pin":26}
    ]
  },
  "datapixels":{
    "freq" : "1",
    "subscribes": [
      {"id":"hall","pin":26},
      {"id":"temp","pin":26}
    ]
  },

  "neopixels":{
    "freq" : "1",
    "subscribes": [
      {"id":"trigger"},
      {"id":"sample_rate"},
      {"id":"x","pin":26},
      {"id":"y","pin":26},
      {"id":"z","pin":26},
      {"id":"length","value":10}
    ]
  },
  "play_sound":{
    "freq" : "1",
    "subscribes": [
      {"id":"trigger"},
      {"id":"sample_rate"},
      {"id":"light","pin":22}
    ]
  },
  "spy_bug":{
    "freq" : "1",
    "events": [
      {"id":"sound","pin":31},
      {"id":"trigger"}
    ],
    "subscribes": [
      {"id":"sample_rate"},
      {"id":"trigger_threshold"}
    ]
  }
}
